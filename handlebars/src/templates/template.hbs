<ul class="users">
  {{#each this}}
    <li class="user">
      <h1 class="user__name">Ім'я: {{name}}</h1>
      <h2 class="user__age">Вік: {{age}}</h2>
      <h3 class="user__email">{{email}}</h3>
      <p class="user__nikcname">{{nickname}}</p>
      <p class="user__hobby">Хобі: {{hobby}}</p>
      <p class="user__skills">Навички: {{skills}}</p>
    </li>
  {{/each}}
</ul>